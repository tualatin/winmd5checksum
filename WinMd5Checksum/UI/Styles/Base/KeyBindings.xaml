<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  >
  <KeyBinding x:Key="OpenFileKeyBinding" Gesture="CTRL+O" />
  <KeyBinding x:Key="SaveFileKeyBinding" Gesture="CTRL+S" />
  <KeyBinding x:Key="StartCalculatingKeyBinding" Gesture="CTRL+R" />
  <KeyBinding x:Key="StopCalculatinKeyBinding" Gesture="CTRL+U" />
  <KeyBinding x:Key="ClearItemsKeyBinding" Gesture="CTRL+E" />
  <KeyBinding x:Key="ToggleAlwaysOnTopKeyBinding" Gesture="ALT+T" />
</ResourceDictionary>
